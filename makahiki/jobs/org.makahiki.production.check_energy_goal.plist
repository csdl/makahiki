<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>Label</key>
    <string>Makahiki: Check Energy Goal (Production)</string>
    <key>WorkingDirectory</key>
    <string>/Users/kukuicup/production/makahiki</string>
    <key>ProgramArguments</key>
    <array>
    <string>/Users/kukuicup/Envs/kukuicup/bin/python</string>
    <string>check_energy_goal.py</string>
    </array>
    
    <!-- Only applies if the task is run as root. -->
    <key>UserName</key>
    <string>kukuicup</string>
    
    <!-- Run this task daily at 11:55 PM -->
    <key>StartCalendarInterval</key>
    <dict>
      <key>Hour</key>
      <integer>23</integer>
      <key>Minute</key>
      <integer>55</integer>
    </dict>
    
    <key>StandardErrorPath</key>
  	<string>/Users/kukuicup/production/log/check-energy-goal.err</string>
  	<key>StandardOutPath</key>
  	<string>/Users/kukuicup/production/log/check-energy-goal.out</string>
</dict>
</plist>
